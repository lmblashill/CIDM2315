namespace homework4;

class Program
{
    static void Main(string[] args)
    {
        Console.WriteLine("Please input integer");
         int N = Convert.ToInt16(Console.ReadLine());
        Console.WriteLine("Please input  second integer");
        int G =Convert.ToInt16(Console.ReadLine());
    if(N >G) {
        Console.WriteLine($"{N} is largest");
    }
    else
        Console.WriteLine($"{G} is largest");
    }
}


    
   

