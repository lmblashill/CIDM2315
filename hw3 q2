using System.Globalization;

namespace Lecture3;

class Program
{
    
    static void Main()
    {
  Console.WriteLine("please input an integer");
  int N = Convert.ToInt16(Console.ReadLine());
  for(int i = 0; i < N; i++)
  {
    for (int j =0; j < N; j++)
    {
        Console.Write("#");
    }
  Console.WriteLine();
  }
}
}
